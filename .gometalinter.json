{
  "Enable": [
    "deadcode",
    "goconst",
    "gocyclo",
    "golint",
    "gosec",
    "ineffassign",
    "misspell",
    "vet",
    "vetshadow",
    "gofmt-"
  ],
  "Linters": {
    "gofmt-": {
      "Command": "gofmt -l",
      "Pattern": "^(?P<path>.*?.go)$",
      "IsFast": true,
      "PartitionStrategy": "directories"
    }
  },
  "Exclude": [
    "declaration of \"err\"",
    "warning: Errors unhandled",
    "should have comment or be unexported",
    "method ToSql should be ToSQL",
    "composite literal uses unkeyed fields",
    "_gen\\.go"
  ],
  "Skip": [
    "pkg/server/skydb/mock_skydb",
    "pkg/server/skydb/pq/migration",
    "vendor"
  ],
  "Cyclo": 10
}
